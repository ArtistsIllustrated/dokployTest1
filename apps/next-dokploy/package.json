{
  "name": "next-dokploy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build-server && npm run build-next",
    "start": "node dist/server.mjs",
    "build-server": "tsx esbuild.config.ts",
    "build-next": "next build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --config ./src/server/db/drizzle.config.ts",
    "dev": "tsx watch -r dotenv/config ./src/server/server.ts  --project tsconfig.server.json ",
    "lint": "next lint",
    "docker:build:canary": "./docker/build.sh canary",
    "docker:push:canary": "./docker/push.sh canary"
  },
  "dependencies": {
    },
  "devDependencies": {
    "@types/node-os-utils": "1.3.4",
    "@types/node-schedule": "2.1.6",
    "@types/bcrypt": "5.0.2",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.17.4",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "drizzle-kit": "^0.21.4",
    "postcss": "^8.4.34",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.2",
    "@types/tar-fs": "2.0.4",
    "@types/qrcode": "^1.5.5",
    "vitest": "^1.6.0",
    "@types/ws": "8.5.10",
    "autoprefixer": "^10.4.14",
    "esbuild": "0.20.2",
    "tsx": "^4.7.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.36.0"
  },
  "packageManager": "pnpm@8.15.4"
}
